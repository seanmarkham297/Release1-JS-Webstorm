<table class="ui table">
  <thead>
    <tr>
      <th>Code</th>
      <th>Temp</th>
      <th>WindSpeed</th>
      <th>Wind Direction</th>
      <th>Pressure</th>
    </tr>
  </thead>
  <tbody>
    {{#each station.readings}}
    <tr>
      <td>
          {{code}}
      </td>
      <td>
          {{temp}}
      </td>
      <td>
          {{windspeed}}
      </td>
      <td>
        {{windDirection}}
      </td>
      <td>
          {{pressure}}
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>